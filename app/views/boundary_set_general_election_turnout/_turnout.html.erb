<% if turnout_iteration.index == 0 -%>
	<%= render :partial => 'turnout_row', :object => turnout %>
<% elsif turnout_iteration.index == @elections.size - 1 -%>
	<%= render :partial => 'turnout_row', :object => turnout %>
<% else -%>
	<% if turnout.constituency_area_name != @elections[turnout_iteration.index - 1].constituency_area_name %>
		<tbody>
			<%= render :partial => 'turnout_row', :object => turnout %>
	<% elsif turnout.constituency_area_name != @elections[turnout_iteration.index + 1].constituency_area_name %>
			<%= render :partial => 'turnout_row', :object => turnout %>
		</tbody>
	<% else -%>
		<%= render :partial => 'turnout_row', :object => turnout %>
	<% end -%>
<% end -%>