<%= render :partial => 'previous_next', :object => @general_election %>

<%= render :partial => 'general_election_english_region/nav' %>

<%= render :partial => 'general_election/counts', :object => @general_election %>



<% unless @elections.empty? %>
	<div class="table-wrapper">
		<table>
			<thead>
				<tr>
					<%= content_tag( 'th', 'Ranking' ) %>
					<%= content_tag( 'th', 'Constituency' ) %>
					<%= content_tag( 'th', 'Winning candidate' ) %>
					<%= content_tag( 'th', 'Winning party' ) %>
					<%= content_tag( 'th', 'Vote share', :class => 'number' ) %>
				</tr>
			</thead>
			<tbody>
				<%= render :partial => 'general_election_vote_share/election', :collection => @elections %>
			</tbody>
		</table>
	</div>
	<div id="downloads">
		<p>
			<%= csv_icon_link(	general_election_english_region_vote_share_list_url( :format => 'csv' ), "Download the data shown on this page: vote share data in England, #{@english_region.name} as a CSV file." ) %>
		</p>
		<p>
			<%= csv_icon_link(	general_election_english_region_candidacy_list_url( :format => 'csv' ), "Download the full data set: candidate-level general election result data in England, #{@english_region.name} as a CSV file." ) %>
		</p>
	</div>
<% end -%>