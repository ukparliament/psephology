<%= render :partial => 'general_election_english_region/nav' %>

<%= render :partial => 'general_election/counts', :object => @general_election %>



<% unless @elections.empty? %>
	<div class="table-wrapper">
		<table>
			<thead>
				<tr>
					<%= content_tag( 'th', 'Ranking' ) %>
					<%= content_tag( 'th', 'Constituency' ) %>
					<%= content_tag( 'th', 'Winning candidate' ) %>
					<%= content_tag( 'th', 'Winning party' ) %>
					<%= content_tag( 'th', 'Vote share', :class => 'number' ) %>
				</tr>
			</thead>
			<tbody>
				<%= render :partial => 'general_election_vote_share/election', :collection => @elections %>
			</tbody>
		</table>
	</div>
<% end -%>