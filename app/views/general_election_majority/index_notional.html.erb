<%= render :partial => 'general_election/nav' %>

<%= render :partial => 'general_election/counts', :object => @general_election %>

<% unless @elections.empty? %>
	<div class="table-wrapper">
		<table id="results" class="sortable sorted-<%= @order %>">
			<%= render :partial => 'table_head_notional' %>
			<tbody>
				<%= render :partial => 'election_notional', :collection => @elections %>
			</tbody>
		</table>
	</div>
<% end -%>

<%= render :partial => 'notional/notice' %>

<% unless @elections.empty? %>
	<div id="downloads">
		<p>
			<%= csv_icon_link(	general_election_majority_list_url( :format => 'csv' ), "Download the data shown on this page: majority data as a CSV file." ) %>
		</p>
		<p>
			<%= csv_icon_link(	general_election_candidacy_list_url( :format => 'csv' ), "Download the full data set: candidate-level general election result data as a CSV file." ) %>
		</p>
	</div>
<% end -%>