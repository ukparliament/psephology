<%= render :partial => 'boundary_set/nav' %>

<% unless @elections.empty? -%>
	<table class="multibody">
		<thead>
			<tr>
				<%= content_tag( 'td', 'Constituency area' ) %>
				<%= content_tag( 'td', 'General election' ) %>
				<%= content_tag( 'td', 'Winning party' ) %>
				<%= content_tag( 'td', 'Vote share', :class => 'number' ) %>
			</tr>
		</thead>
		<tbody>
			<%= render :partial => 'vote_share', :collection => @elections, cached: true %>
		</tbody>
	</table>
<% end -%>