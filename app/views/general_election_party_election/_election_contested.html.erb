<tr>
	<%= content_tag( 'td', link_to( election_contested.constituency_name, election_show_url( :election => election_contested ) ) ) %>
	<% if election_contested.member_mnis_id -%>
		<%= content_tag( 'td', link_to( election_contested.candidate_polling_name, member_show_url( :member => election_contested.member_mnis_id ) ) ) %>
	<% else -%>
		<%= content_tag( 'td', election_contested.candidate_polling_name ) %>
	<% end -%>
	<%= content_tag( 'td', election_contested.electorate_population_count.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) %>
	<%= content_tag( 'td', "#{election_contested.turnout}%", :class => 'number' ) %>
	<%= content_tag( 'td', election_contested.candidacy_vote_count.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) %>
	<%= content_tag( 'td', "#{( election_contested.candidacy_vote_share.to_f * 100).round( 1 ) }%", :class => 'number' ) %>
	

	<% if election_contested.candidacy_vote_change -%>
		<%= content_tag( 'td', "#{( election_contested.candidacy_vote_change.to_f * 100 ).round( 1 )}%", :class => 'number' ) %>
	<% else -%>
		<%= content_tag( 'td', '-', :class => 'number' ) %>
	<% end -%>
	
	
	
	
	
	<%= content_tag( 'td', election_contested.candidacy_result_position.ordinalize, :class => 'number' ) %>
</tr>