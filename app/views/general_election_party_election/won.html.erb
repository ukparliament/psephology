<%= render :partial => 'nav' %>

<% if @elections_won.empty? %>
	<%= content_tag( 'p', 'No elections won.' ) %>
<% else -%>
	<table>
		<thead>
			<tr>
				<%= content_tag( 'td', 'Constituency' ) %>
				<%= content_tag( 'td', 'Winning candidate' ) %>
				<%= content_tag( 'td', 'Vote' ) %>
				<%= content_tag( 'td', 'Vote share' ) %>
				<%= content_tag( 'td', 'Vote change' ) %>
				<%= content_tag( 'td', 'Majority' ) %>
			</tr>
		</thead>
		<tbody>
			<%= render :partial => 'election_won', :collection => @elections_won %>
		</tbody>
	</table>
<% end -%>