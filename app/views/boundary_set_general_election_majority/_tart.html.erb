<tr>
	<%= content_tag( 'td', link_to( tart.constituency_area_name, constituency_area_show_url( :constituency_area => tart.constituency_area_id ) ) ) %>
	<%= content_tag( 'td', link_to( tart.general_election_polling_on.strftime( $DATE_DISPLAY_FORMAT ), general_election_show_url( :general_election => tart.general_election_id ) ) ) %>
	<td>
		<% if tart.winning_candidacy_standing_as_commons_speaker -%>
			<%= content_tag( 'abbr', 'Spk', :title => 'Speaker' ) %>
		<% elsif tart.winning_candidacy_standing_as_independent -%>
			<%= content_tag( 'abbr', 'Ind', :title => 'Independent' ) %>
		<% elsif tart.winning_candidacy_adjunct_party_name -%>
			<%= "#{content_tag( 'abbr', tart.winning_candidacy_party_abbreviation, :title => tart.winning_candidacy_party_name )} / #{content_tag( 'abbr', tart.winning_candidacy_adjunct_party_abbreviation, :title => tart.winning_candidacy_adjunct_party_name )}".html_safe %>
		<% else -%>
			<%= content_tag( 'abbr', tart.winning_candidacy_party_abbreviation, :title => tart.winning_candidacy_party_name ) %>
		<% end -%>
	</td>
	<%= content_tag( 'td', tart.majority.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) %>
	<%= content_tag( 'td', tart.valid_vote_count.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) %>
	<%= content_tag( 'td', link_to( "#{( tart.majority_percentage * 100 ).round( 1 )}%", election_show_url( :election => tart ) ), :class => 'number' ) %>
</tr>