<%= render :partial => 'nav' %>


<% if @constituency_areas.empty? -%>
	<%= content_tag( 'p', 'Constituency area information is not yet available.' ) %>
<% else -%>
	<div id="constituencies">
		<%= content_tag( 'h2', 'Constituency areas' ) %>
		<ol>
			<%= render :partial => 'constituency_area/constituency_area', :collection => @constituency_areas %>
		</ol>
	</div>
<% end -%>