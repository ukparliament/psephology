<script type="application/ld+json">
    {
		"@context": "https://schema.org",
		"@type": "BreadcrumbList",
		"itemListElement": [
	  		{
	        	"@type": "ListItem",
	        	"position": 1,
	        	"name": "UK Parliament",
	        	"item": "https://www.parliament.uk/"
	      	 },
			 {
				 "@type": "ListItem",
				 "position": 2,
				 "name": "House of Commons Library",
				 "item": "https://commonslibrary.parliament.uk/"
			 },
		  	 <% if crumb.empty? -%>
		  		{
	        		"@type": "ListItem",
	        		"position": 3,
	        		"name": "Election Results"
	      	  	}
			<% else -%>
				{
	        		"@type": "ListItem",
	        		"position": 3,
	        		"name": "Election Results",
				 	"item": "<%= home_url %>"
	      	  	}
				<%= render :partial => 'layouts/head/json_ld/crumb_item', :collection => crumb %>
			<% end -%>
		]
    }
</script>