<div id="candidacies">
	<%= content_tag( 'h2', 'Candidates' ) %>
	<div class="table-wrapper">
		<table>
			<thead>
				<tr>
					<%= content_tag( 'th', 'Name' ) %>
					<%= content_tag( 'th', 'Party' ) %>
					<%= content_tag( 'th', 'Vote', :class => 'number' ) %>
					<%= content_tag( 'th', 'Vote share', :class => 'number' ) %>
					<%= content_tag( 'th', 'Vote change', :class => 'number' ) %>
			</thead>
			<tbody>
				<%= render :partial => 'result', :collection => results_candidacies %>
			</tbody>
		</table>
	</div>
</div>