					<%= content_tag( 'td', 'Description' ) %>
				</tr>
			</thead>
			<tbody>
				<tr>
					<%= content_tag( 'td', 'id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'The primary key for the table.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'boundary_set_id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', link_to( 'boundary_sets', '#boundary_sets' ) ) %>
					<%= content_tag( 'td', 'Identifies the boundary set.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'legislation_item_id' ) %>
