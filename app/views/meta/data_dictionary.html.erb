<div id="boundary_set_general_election_party_performances">
	<%= content_tag( 'h2', 'boundary_set_general_election_party_performances' ) %>
	<%= content_tag( 'p', "A denormalised table storing performances for a political party, in a general election, in a boundary set. Used to make #{link_to( 'boundary set general election party performance pages', 'https://electionresults.parliament.uk/boundary-sets/6/parties' )}.".html_safe ) %>

	<div class="table-wrapper">
		<table>
			<thead>
				<tr>
					<%= content_tag( 'td', 'Field name' ) %>
					<%= content_tag( 'td', 'Field type' ) %>
					<%= content_tag( 'td', 'Can be null?' ) %>
					<%= content_tag( 'td', 'Foreign key to' ) %>
					<%= content_tag( 'td', 'Description' ) %>
				</tr>
			</thead>
			<tbody>
				<tr>
					<%= content_tag( 'td', 'id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'The primary key for the table.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'constituency_contested_count' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'The number of constituencies contested by candidates standing for a given political party in a given general election in a given boundary set. This is not used by the election results website, which only lists the number of constituencies won.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'constituency_won_count' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'The number of constituencies won by candidates standing for a given political party in a given general election in a given boundary set.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'cumulative_vote_count' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'The cumulative vote count of all candidates standing for a given political party in a given general election in a given boundary set. This is not used by the election results website, which only lists the number of constituencies won.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'general_election_id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', link_to( 'general_elections', '#general_elections' ) ) %>
					<%= content_tag( 'td', 'Identifies the general election.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'political_party_id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', link_to( 'political_parties', '#political_parties' ) ) %>
					<%= content_tag( 'td', 'Identifies the political party.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'boundary_set_id' ) %>
					<%= content_tag( 'td', 'Integer' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', link_to( 'boundary_sets', '#boundary_sets' ) ) %>
					<%= content_tag( 'td', 'Identifies the boundary set.' ) %>
				</tr>
			</tbody>
		</table>
	</div>
</div>

