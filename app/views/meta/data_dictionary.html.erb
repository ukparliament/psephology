				</tr>
				<tr>
					<%= content_tag( 'td', 'candidate_is_sitting_mp' ) %>
					<%= content_tag( 'td', 'Boolean' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
					<%= content_tag( 'td', 'Used to record if the candidate was a sitting MP at the time of the election or a Member of Parliament in the Parliament preceding a general election.' ) %>
				</tr>
				<tr>
					<%= content_tag( 'td', 'candidate_is_former_mp' ) %>
					<%= content_tag( 'td', 'Boolean' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
