				</tr>
				<tr>
					<%= content_tag( 'td', 'is_standing_as_commons_speaker' ) %>
					<%= content_tag( 'td', 'Boolean' ) %>
					<%= content_tag( 'td', 'No' ) %>
					<%= content_tag( 'td', '' ) %>
