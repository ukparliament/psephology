<%= render :partial => 'counts', :object => @general_election %>

<%= render :partial => 'nav' %>

<% unless @elections.empty? %>

	<div id="countries">
		<%= content_tag( 'h2', 'Elections by country' ) %>
		<ol>
			<%= render :partial => 'general_election_country/country', :collection => @countries %>
		</ol>
	</div>
	
	<% if FEATURE_SHOW_GENERAL_ELECTION_CARTOGRAMS -%>
		<div id="iframe-holder"></div>
		<script>
			var holder = document.getElementById("iframe-holder");
			var iframe = document.createElement('iframe');
			iframe.src = '/cartogram/general-elections/<%= @general_election.polling_on %>-notional/uk.html';
			iframe.loading = 'lazy';
			iframe.id = 'cartogram';
			holder.appendChild(iframe);
		</script>
	<% end -%>
	
	<div id="all">
		<%= content_tag( 'h2', 'All elections' ) %>
		<ol>
			<%= render :partial => 'election/election', :collection => @elections %>
		</ol>
	</div>
<% end -%>

<%= render :partial => 'notional/notice' %>

<div id="downloads">
	<p>
		<%= csv_icon_link(	general_election_election_list_url( :format => 'csv' ), "Download the data shown on this page: election data as a CSV file." ) %>
	</p>
	<p>
		<%= csv_icon_link(	general_election_candidacy_list_url( :format => 'csv' ), "Download the full data set: candidate-level general election result data as a CSV file." ) %>
	</p>
</div>