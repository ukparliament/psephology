<p class="subnav">

	<%# Notional general election results %>
	<% if @general_election.is_notional -%>
		<%= link_to( 'Parties', general_election_show_url, :class => 'parties' ) %>
		<%= link_to( 'Constituencies', general_election_constituency_area_list_url, :class => 'constituency-areas' ) %>
		<%= link_to( 'Map', general_election_map_url, :class => 'map' )  %>
		<%= link_to( 'Majorities', general_election_majority_list_url, :class => 'majorities' ) %>
		<%= link_to( 'Vote shares of winning candidates', general_election_vote_share_list_url, :class => 'vote-shares' ) %>
		<%= link_to( 'Turnouts', general_election_turnout_list_url, :class => 'turnouts' ) %>
		
	<%# Real general election results %>
	<% else -%>
		<%= link_to( 'Parties', general_election_show_url, :class => 'parties' ) %>
		<%= link_to( 'Constituencies', general_election_constituency_area_list_url, :class => 'constituency-areas' ) %>
		
		<%# Show the map if winners are known %>
		<% if @general_election.publication_state > 1 %>
			<%= link_to( 'Map', general_election_map_url, :class => 'map' )  %>
		<% end -%>
		
		<%# Show stats breakdown when all numbers are known %>
		<% if @general_election.publication_state == 3 %>
			<%= link_to( 'Majorities', general_election_majority_list_url, :class => 'majorities' ) %>
			<%= link_to( 'Vote shares of winning candidates', general_election_vote_share_list_url, :class => 'vote-shares' ) %>
			<%= link_to( 'Turnouts', general_election_turnout_list_url, :class => 'turnouts' ) %>
			<%= link_to( 'Declaration times', general_election_declaration_time_list_url, :class => 'declaration-times' ) %>
			<%= link_to( 'Non-party candidates', general_election_uncertified_candidacy_list_url, :class => 'uncertified-candidacies' ) %>
		<% end -%>
		<%= link_to( 'Boundary sets', general_election_boundary_set_list_url, :class => 'boundary-sets' ) %>
	<% end -%>
</p>