<tr>
	<%= content_tag( 'td', link_to( party_performance.party_name, political_party_show_url( :political_party => party_performance.political_party_id )) ) %>
	<%= content_tag( 'td', link_to( party_performance.constituency_contested_count, general_election_party_election_list_url( :political_party => party_performance.political_party_id ), :title => "#{party_performance.constituency_contested_count} elections contested by #{party_performance.party_name}" ), :class => 'number' ) %>
	<%= content_tag( 'td', party_performance.cumulative_vote_count.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) %>
	<%= content_tag( 'td', "#{party_performance.vote_share}%", :class => 'number' )%>
	<%= content_tag( 'td', link_to( party_performance.constituency_won_count, general_election_party_election_won_url( :political_party => party_performance.political_party_id ), :title => "#{party_performance.constituency_won_count} elections won by #{party_performance.party_name}" ), :class => 'number' ) %>
</tr>